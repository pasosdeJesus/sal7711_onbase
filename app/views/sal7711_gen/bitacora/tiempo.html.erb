<div class="page-header">
  <h1><%=t '.title', :default => "Consultas en el tiempo" %></h1>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <%= @tiempocons.count %> grupos de consultas en intervalos de 
    <%= @intervalo%> segundos entre 
    <%= @fechaini %> y <%= @fechafin %>
  </div>
  <div class="panel-body">

    <table class="table table-striped">
      <thead>
        <tr><th>Hora</th><th>Usuarios</th><th>Consultas</th><th>Presentar Art√≠culo</th>
      </thead>
      <tbody>
        <% @tiempocons.try(:each) do |u| %>
          <tr>
            <td><%= u[0] %></td>
            <td><%= u[1] %></td>
            <td><%= u[2] %></td>
            <td><%= u[3] %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
